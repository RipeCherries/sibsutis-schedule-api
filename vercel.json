{
  "builds": [
    {
      "src": "app/admin/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "app/api/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/admin/(.*)",
      "dest": "app/admin/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "app/api/$1"
    }
  ]
}