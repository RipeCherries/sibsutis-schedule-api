{
  "builds": [
    {
      "src": "admin/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "api/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/admin/(.*)",
      "dest": "admin/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/$1"
    }
  ]
}